<% title "Log In" %>

<div class="container signup-padding">
  <div class="row">
    <div class="col-sm-6 col-md-4 col-md-offset-4">
      <h1 class="text-center">Log In</h1>
       <div class="account-wall">

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <%= f.label :email, :class => 'h5 materialform' %><br />
    <%= f.email_field :email, autofocus: true, :class => 'form-control materialform' %>

    <%= f.label :password, :class => 'h5 materialform' %><br />
    <%= f.password_field :password, autocomplete: "off", :class => 'form-control materialform' %>

              <div class="materialformbutton">
  <% if devise_mapping.rememberable? -%>
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
  <% end -%>
  </div>

  <div class="actions">
     <div class="materialformbutton">
    <%= f.submit "Log in", :class => 'btn btn-main' %>
  </div>
    </div>
<% end %>
</div>
<p class="have-account-padding">Need an account?<%= render "devise/shared/links" %></p>
</div>
</div>
</div>